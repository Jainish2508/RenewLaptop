@{
    ViewBag.Title = "Pricing";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<!-- inner page banner -->
<div id="inner_banner" class="section inner_banner_section">
    <div class="container">
        <div class="row">
            <div class="col-md-12">
                <div class="full">
                    <div class="title-holder">
                        <div class="title-holder-cell text-left">
                            <h1 class="page-title">Pricing</h1>
                            <ol class="breadcrumb">
                                <li><a href="@Url.Action("index","home")">Home</a></li>
                                <li class="active">Pricing</li>
                            </ol>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
<!-- end inner page banner -->
<!-- section -->
<div class="section padding_layout_1 service_list">
    <div class="container">
        <div class="row">
            <div class="col-md-12">
                <div class="full">
                    <div class="main_heading text_align_center">
                        <h2>Our Pricing Plans</h2>
                        <p class="large"> for Laptop Servicing</p>
                    </div>
                </div>
            </div>
            <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
                <!-- full price table -->
                <div class="price_table row" style="margin-top: 0;">
                    <!-- price table one -->
                    <div class="col-lg-4 col-md-4 col-sm-12 col-xs-12">
                        <div class="price_table_inner margin_bottom_30">
                            <div class="price_head text_align_center">
                                <h5>Diagnostic</h5>
                                <br />
                                <p>Never be charged for just finding the problems!</p>
                            </div>
                            <div class="price_contant">
                                <div class="center">
                                    <p class="price_amount"> <span class="price_no">FREE</span></p>
                                </div>
                                <div class="price_cont text_align_center">
                                    <p>Laptop Health Checkup</p>
                                    <p>Hardware & Software Diagnosis</p>
                                    <p>Virus & Spyware Scan</p>
                                    <p>Disk Analyzation</p>
                                    <p>Walk-In Only</p>
                                    <p>Only for Laptop</p>
                                </div>
                            </div>
                            <div class="price_bottom text_align_center">
                                <div class="center"> <a href="@Url.Action("checkout","pricing", new { product_id = 1})" class="btn dark_gray_bt">Book Now</a> </div>
                            </div>
                        </div>
                    </div>
                    <!-- end price table one -->
                    <!-- price table two -->
                    <div class="col-lg-4 col-md-4 col-sm-12 col-xs-12 price_table_active">
                        <div class="row">
                            <div class="price_table_inner margin_bottom_30">
                                <div class="price_head text_align_center white_fonts">
                                    <h5>Annual Support</h5>
                                    <br />
                                    <p>Get your Laptop Issues repaired for complete 1 year with 15 days money back guarantee !</p>
                                </div>
                                <div class="price_contant">
                                    <div class="center">
                                        <p class="price_amount">₹ <span class="price_no">4800</span>/-<br />(Inc. GST)</p>
                                    </div>
                                    <div class="price_cont text_align_center">
                                        <p>All services of Diagnostic</p>
                                        <p>All services of One-Time Fix</p>
                                        <p>Dedicated Technician</p>
                                        <p>Priority Service</p>
                                        <p>Monthly Laptop Health Checkup</p>
                                        <p>Wlak-In or Location(2-3 KMs)</p>
                                        <p>Short Processor Can't be Repaired</p>
                                    </div>
                                </div>
                                <div class="price_bottom text_align_center">
                                    <div class="center"> <a href="tel:+91 99740 97022" class="btn main_bt">Purchase Now</a> </div>
                                </div>
                            </div>
                        </div>
                    </div>
                    <!-- end price table two -->
                    <!-- price table three -->
                    <div class="col-lg-4 col-md-4 col-sm-12 col-xs-12">
                        <div class="price_table_inner">
                            <div class="price_head text_align_center">
                                <h5>One-Time Fix</h5>
                                <p>Fix your PC Issues for One Time with the Validity of 15 Days!</p>
                            </div>
                            <div class="price_contant">
                                <div class="center">
                                    <p class="price_amount">₹ <span class="price_no">2800</span>/-<br />(Inc. GST)</p>
                                </div>
                                <div class="price_cont text_align_center">
                                    <p>All services of Diagnostic</p>
                                    <p>Laptop Tune up & Optimization</p>
                                    <p>Fix Within 24hrs</p>
                                    <p>Fully trained employees</p>
                                    <p>Walk-In Only</p>
                                    <p>Only for Laptop</p>
                                </div>
                            </div>
                            <div class="price_bottom text_align_center">
                                <div class="center"> <a href="tel:+91 99740 97022" class="btn dark_gray_bt">Purchase Now</a> </div>
                            </div>
                        </div>
                    </div>
                    <!-- end price table three -->
                </div>
                <!-- end full price table -->
            </div>
        </div>
        <div class="row padding_layout_1_small" style="padding-bottom:0px !important">
            <div class="col-md-12">
                <div class="text_align_center">
                    <p>* Terms & Conditions Apply</p>
                </div>
            </div>
        </div>
    </div>
</div>
<!-- end section -->