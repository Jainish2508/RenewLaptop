@{
    ViewBag.Title = "About Us";
    Layout = "~/Views/Shared/_Layout.cshtml";
    ViewBag.Description = "Get your Laptops and Computers get repaired at a very low price with the most leading, fastest, and promised repair service station in Ahmedabad near Satelite.";
}

<!-- inner page banner -->
<div id="inner_banner" class="section inner_banner_section">
    <div class="container">
        <div class="row">
            <div class="col-md-12">
                <div class="full">
                    <div class="title-holder">
                        <div class="title-holder-cell text-left">
                            <h1 class="page-title">About Us</h1>
                            <ol class="breadcrumb">
                                <li><a href="@Url.Action("index","home")">Home</a></li>
                                <li class="active">About Us</li>
                            </ol>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
<!-- end inner page banner -->
<!-- section -->
<div class="section padding_layout_1">
    <div class="container">
        <div class="row">
            <div class="col-md-12">
                <div class="full">
                    <div class="main_heading text_align_center">
                        <h2>We are Leading Company</h2>
                        <p class="large">Fastest repair service with best price!</p>
                    </div>
                </div>
            </div>
        </div>
        <div class="row about_blog">
            <div class="col-lg-8 col-md-8 col-sm-12 about_cont_blog">
                <div class="full text_align_left">
                    <h3>What we do?</h3>
                    <p>
                        We are leading and most promised Computer Repairing Service station in Ahmedabad, Gujarat. In our 9 years of career we have more than 1000 100% satisfied customers..
                    </p>
                    <ul>
                        <li>
                            <i class="fa fa-check-circle" style="display:inline-block"></i>
                            Hardware installations & Upgrading memory, hard-drives, SSDs, Video
                            Cards, Hard-Drives etc.
                        </li>
                        <li>
                            <i class="fa fa-check-circle"></i>Computer Peripheral installation, Configuration, and networking, including
                            printers.
                        </li>
                        <li><i class="fa fa-check-circle"></i>Network Design and Configuration.</li>
                        <li>
                            <i class="fa fa-check-circle"></i>Software Installation and Upgrades, such as operating system and database
                            application.
                        </li>
                        <li><i class="fa fa-check-circle"></i>Full Data-Backup and Recovery Solutions.</li>
                    </ul>
                </div>
            </div>
            <div class="col-lg-4 col-md-4 col-sm-12 about_feature_img padding_right_0 top_80">
                <div class="full text_align_center"> <img class="img-responsive" src="~/Content/images/it_service/post-06.jpg" alt="#" /> </div>
            </div>
        </div>
    </div>
</div>
<!-- end section -->
<!-- section -->
<div class="section padding_layout_1 light_silver gross_layout right_gross_layout">
    <div class="container">
        <div class="row">
            <div class="col-md-12">
                <div class="full">
                    <div class="main_heading text_align_right">
                        <h2>Specialized Service</h2>
                        <p class="large">Easy and effective way to get your device repaired.</p>
                    </div>
                </div>
            </div>
        </div>
        <div class="row" style="margin-top: 35px">
            <div class="col-md-5"> </div>
            <div class="col-md-7">
                <div class="row">
                    <div class="full margin_bottom_30">
                        <div class="accordion border_circle">
                            <div class="bs-example">
                                <div class="panel-group" id="accordion">
                                    <div class="panel panel-default">
                                        <div class="panel-heading">
                                            <p class="panel-title"> <a data-toggle="collapse" data-parent="#accordion" href="#collapseOne"><i class="fa fa-apple" aria-hidden="true"></i> Specialized in repairing Apple computers and MacBook<i class="fa fa-angle-down"></i></a> </p>
                                        </div>
                                        <div id="collapseOne" class="panel-collapse collapse in show">
                                            <div class="panel-body">
                                                <p>
                                                    Our professional and experienced experts can repair and service
                                                    all brands of laptops including the Apple Macbook. We take pride in providing top-notch
                                                    quality services at very reasonable prices. Even the customers from remote areas can
                                                    avail of our desktop laptop repair services through our remote desktop support services.
                                                </p>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="panel panel-default">
                                        <div class="panel-heading">
                                            <p class="panel-title"> <a data-toggle="collapse" data-parent="#accordion" href="#collapseTwo"><i class="fa fa-laptop"></i> Laptop and Computer Repair Station<i class="fa fa-angle-down"></i></a> </p>
                                        </div>
                                        <div id="collapseTwo" class="panel-collapse collapse">
                                            <div class="panel-body">
                                                <p>
                                                    The certified technicians of Renew-Laptop are dedicated to providing superior quality
                                                    laptop repair services.Our technical experts deliver 24x7 best desktop and laptop repair services as soon as
                                                    you contact us. You can interact with our experts who can quickly fix your laptop
                                                    problems in a couple of minutes which saves our customers valuable time and money.
                                                </p>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
<!-- end section -->
